#include<bits/stdc++.h>
using namespace std;

vector<int> output(vector<int> &input){
	
	int n=input.size();

for(int i=0;i<n/2;i++){
	
	swap(input[i],input[n-1-i]);
}
	
	for(int i=1;i<n;i+=2){
		
		int a=input[n-1];
		
		for(int j=n-1;j>i;j--){
			input[j]=input[j-1];
		}
		
		input[i]=a;
	}
	return input;
}

int main(){
	
	vector<int> input;

	input.push_back(2);
	input.push_back(4);
	input.push_back(6);
	input.push_back(8);
	input.push_back(10);
	


int n=input.size();

output(input);
for(int i=0;i<n;i++){

	cout<<input[i]<<" ";
}
	return 0;
}

//input[2,4,6,8,10]
//output[10,2,8,4,6]
